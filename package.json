{
  "name": "vue-abd-slider",
  "version": "0.0.3",
  "private": false,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "serve:lib": "vue-cli-service serve",
    "build:lib": "vue-cli-service build --target lib --name vue-abd-slider ./src/install.js",
    "publish:lib": "npm run build:lib && npm version patch -m \"build: release %s\" && npm publish --access public",
    "preview": "vite preview",
    "test:unit": "vitest --silent --environment jsdom --root src/",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@vue/cli-service": "^5.0.8",
    "vue": "^3.2.45",
    "vue-abd-slider": "^0.0.3"
  },
  "main": "./dist/vue-abd-slider.common.js",
  "files": [
    "dist/*"
  ],
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^12.0.2",
    "eslint": "^8.22.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "start-server-and-test": "^1.15.2",
    "typescript": "~4.7.4",
    "vite": "^4.0.0",
    "vitest": "^0.25.6",
    "vue-tsc": "^1.0.12"
  }
}
